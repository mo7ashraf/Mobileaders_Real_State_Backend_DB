    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
